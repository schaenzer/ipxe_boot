#!ipxe


echo ${uuid}

set debian_mirror http://deb.debian.org/
set dir debian/dists/Debian11.1/main/installer-amd64/current/images/netboot/

imgfree
kernel ${debian_mirror}/${dir}/linux ${install_params} ${netcfg} ${mirrorcfg} initrd=initrd.magic ${cmdline}
initrd ${debian_mirror}/${dir}/initrd.gz
echo
echo MD5sums:
md5sum linux initrd.gz
boot
